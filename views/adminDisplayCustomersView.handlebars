		<a type="submit" class="btn btn-outline-primary" href="/admin">Admin Menu</a>
	</nav>

	<div class="table-responsive">
		<table class="table table-condensed table table-bordered table-striped">
			<thead>
				<tr>
					<th>Customers</th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>
							<a href="/admin/customer/{{this.id}}">{{this.name}}</a>
						</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
